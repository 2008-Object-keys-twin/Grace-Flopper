{
  "env": {
    "browser": true,
    "es6": true,
    "commonjs": true,
    "node": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "plugin:prettier/recommended",
    "prettier/react",
    "plugin:react-redux/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020
  },
  "plugins": ["react", "prettier", "react-redux"],
  "rules": {
    "no-unused-vars": 0,
    "prettier/prettier": "error"
  }
}
